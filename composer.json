{
    "name": "simple-as-fuck/laravel-lock",
    "description": "Laravel integration for symfony/lock.",
    "require": {
        "php": "^7.4|^8.0",
        "symfony/lock": "^5.3",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.92",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {"SimpleAsFuck\\LaravelLock\\": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleAsFuck\\LaravelLock\\Provider\\PackageProvider"
            ]
        }
    }
}
